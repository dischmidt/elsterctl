# Copy this file to .env and adjust values for your local setup.

# ERiC runtime configuration
ELSTER_ERIC_LIB=/absolute/path/to/vendor/eric/runtime/ERiC-43.3.2.0/Darwin-universal/lib/libericapi.dylib
DYLD_LIBRARY_PATH=/absolute/path/to/vendor/eric/runtime/ERiC-43.3.2.0/Darwin-universal/lib

# ELSTER certificate and identity
ELSTER_CERT_PIN=change-me
ELSTER_DEFAULT_CERTIFICATE=/absolute/path/to/certs/test-soft-pse.pfx
ELSTER_HERSTELLER_ID=change-me

# Safety and defaults
ELSTERCTL_FORCE_TEST_MODE=1
ELSTER_DEFAULT_TRANSFER_MODE=test
ELSTER_DEFAULT_DATA_TYPE_VERSION=TH11
ELSTER_DEFAULT_PIN_ENV=ELSTER_CERT_PIN

# Optional logging
ELSTER_LOG_LEVEL=INFO
